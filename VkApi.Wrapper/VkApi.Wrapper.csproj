<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <ApplicationIcon></ApplicationIcon>
    <RepositoryUrl>https://github.com/inredikawb/VkLibrary</RepositoryUrl>
    <Version>1.2.5</Version>
    <Authors>Artyom G, Alexey I and Contributors</Authors>
    <Company>VkApi.Wrapper</Company>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Description>VkLibrary is an unofficial vk.com client library implemented in C#. It covers all existing vk.com API methods and types, contains helper methods for direct auth and OAuth, contains methods for easy file uploading to vk.com servers using post requests.</Description>
    <PackageProjectUrl>https://github.com/inredikawb/VkLibrary</PackageProjectUrl>
    <PackageTags>vk vk.com vkontakte vkontakte.ru api sdk library</PackageTags>
    <Copyright>Copyright Â© 2020</Copyright>
    <PackageReleaseNotes>Add support for Bot long poll</PackageReleaseNotes>
    <AssemblyVersion>1.2.5.0</AssemblyVersion>
    <FileVersion>1.2.5.0</FileVersion>
    <AssemblyName>VkApi.Wrapper</AssemblyName>
    <RootNamespace>VkApi.Wrapper</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.1\VkLibrary.Core.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.1\VkLibrary.Core.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>
</Project>